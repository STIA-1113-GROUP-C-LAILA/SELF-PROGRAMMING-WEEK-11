public class TestArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a [] = new int [5];
		int i;
		
		a[0] = 10;
		a[1] = 20;
		a[2] = 70;
		a[3] = 40;
		a[4] = 50;
		
